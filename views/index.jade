extends layout

block content
  #social
    ul
      li.fb
        div.fb-like(data-href="https://developers.facebook.com/docs/plugins/", data-layout="button_count", data-action="like", data-show-faces="false", data-share="false")
      li.st
        su:badge(layout="1")
      li.tw
        a.twitter-share-button(href="https://twitter.com/share", data-lang="en") Tweet
        script.
          !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="https://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

  #cat(data-size=1046)
    div.hint
      //h2 Piccsy Playground Presents
      h1 Cat & Mouse
      p Point your mouse anywhere inside this window
    div.wait
      p Finding Mouse... Hold Still!
      div.progress
        div.bar

  #fb-root

  script.
    (function(d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (d.getElementById(id)) return; js = d.createElement(s); js.id = id; js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=192271934223559"; fjs.parentNode.insertBefore(js, fjs); }(document, 'script', 'facebook-jssdk'));

  script.
    (function() { var li = document.createElement('script'); li.type = 'text/javascript'; li.async = true; li.src = ('https:' == document.location.protocol ? 'https:' : 'http:') + '//platform.stumbleupon.com/1/widgets.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(li, s); })();

  .cloud.cloud1cat(style="top: 500px; left: 50px;")
  .cloud.cloud2cat(style="top: 50px; left: 500px;")
